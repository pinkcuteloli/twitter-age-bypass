{
  "manifest_version": 3,
  "name": "Twitter Age Restriction Bypass",
  "version": "1.0",
  "host_permissions": ["https://x.com/*"],
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["loader.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": ["https://x.com/*"]
    }
  ]
}
